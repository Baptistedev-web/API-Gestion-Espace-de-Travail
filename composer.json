{
    "name": "espace-travail/api-restful",
    "description": "API for Espace de Travail",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/doctrine-orm": ">=4.1.7",
        "api-platform/symfony": ">=4.1.7",
        "doctrine/annotations": ">=2.0.2",
        "doctrine/dbal": "^3.9.4",
        "doctrine/doctrine-bundle": ">=2.14",
        "doctrine/doctrine-migrations-bundle": ">=3.4.1",
        "doctrine/orm": ">=3.3.3",
        "doctrine/persistence": ">=3.4",
        "egulias/email-validator": ">=4.0.4",
        "lexik/jwt-authentication-bundle": ">=3.1.1",
        "nelmio/cors-bundle": ">=2.5",
        "phpdocumentor/reflection-docblock": ">=5.6.2",
        "phpstan/phpdoc-parser": ">=2.1",
        "symfony/asset": ">=7.2",
        "symfony/console": ">=7.2.6",
        "symfony/dotenv": ">=7.2",
        "symfony/expression-language": ">=7.2",
        "symfony/flex": ">=2.5",
        "symfony/framework-bundle": ">=7.2.5",
        "symfony/monolog-bundle": ">=3.10",
        "symfony/property-access": ">=7.2.3",
        "symfony/property-info": ">=7.2.5",
        "symfony/runtime": ">=7.2.3",
        "symfony/security-bundle": ">=7.2.3",
        "symfony/serializer": ">=7.2.6",
        "symfony/translation": ">=7.2.6",
        "symfony/twig-bundle": ">=7.2",
        "symfony/validator": ">=7.2.6",
        "symfony/yaml": ">=7.2.6"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": ">=4.1",
        "friendsofphp/php-cs-fixer": ">=3.75",
        "phpstan/phpstan": ">=2.1.14",
        "phpunit/php-code-coverage": "^10.1.16",
        "phpunit/phpunit": "^10.5.46",
        "symfony/browser-kit": ">=7.2.4",
        "symfony/css-selector": ">=7.2",
        "symfony/maker-bundle": ">=1.63",
        "symfony/phpunit-bridge": ">=7.2.6",
        "zenstruck/foundry": ">=2.4.3"
    }
}
